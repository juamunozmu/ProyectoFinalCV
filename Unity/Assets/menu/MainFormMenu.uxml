<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" editor-extension-mode="False">
    <Style src="project://database/Assets/menu/StyleSheet.uss?fileID=7433441132597879392&amp;guid=09a52e9638c4675faa15ce40998f142b&amp;type=3#StyleSheet"/>
    <ui:VisualElement name="Root" class="root-container">
        <ui:VisualElement name="ContentArea" class="content-area">
            <ui:ScrollView name="View_Home" class="app-view view-active">
                <ui:VisualElement class="padding-container">
                    <ui:Label text="Hola, que deseas aprender hoy?" class="header-title"/>
                    <ui:VisualElement class="card">
                        <ui:Label text="SE&#209;A DEL D&#205;A" class="card-title"/>
                        <ui:VisualElement class="media-placeholder">
                            <ui:Label text="[ 3D MODEL ]" style="color: rgb(150, 150, 150);"/>
                        </ui:VisualElement>
                        <ui:VisualElement style="flex-direction: row; justify-content: flex-end; margin-top: 10px;">
                            <ui:Label text="Today" class="tag-accent"/>
                        </ui:VisualElement>
                    </ui:VisualElement>
                </ui:VisualElement>
            </ui:ScrollView>
            <ui:ScrollView name="View_Dictionary" class="app-view">
                <ui:VisualElement class="padding-container">
                    <ui:Label text="Dictionario" class="header-title"/>
                    <ui:VisualElement class="card">
                        <ui:Label text="Abecedario" class="card-title"/>
