                        <ui:Button text="Letra B" class="list-item"/>
                    </ui:VisualElement>
                </ui:VisualElement>
            </ui:ScrollView>
            <ui:ScrollView name="View_Lessons" class="app-view">
                <ui:VisualElement class="padding-container">
                    <ui:Label text="Lecciones" class="header-title"/>
                    <ui:Button text="1. Saludos" class="lesson-btn"/>
                    <ui:Button text="2. Familia" class="lesson-btn"/>
                </ui:VisualElement>
            </ui:ScrollView>
            <ui:ScrollView name="View_Practice" class="app-view">
                <ui:VisualElement class="padding-container">
                    <ui:Label text="Practicar" class="header-title"/>
                    <ui:VisualElement class="card" style="align-items: center; padding: 40px;">
                        <ui:Label text="Ready to start?" class="card-title"/>
                        <ui:Button text="Start Matching" class="practice-btn"/>
                    </ui:VisualElement>
                </ui:VisualElement>
            </ui:ScrollView>
        </ui:VisualElement>
        <ui:VisualElement name="BottomMenuBar" class="menu-bar">
            <ui:Button name="Btn_Home" text="HOME" class="menu-btn active"/>
            <ui:Button name="Btn_Dictionary" text="DICTIONARIO" class="menu-btn"/>
            <ui:Button name="Btn_Lessons" text="LECCIONES" class="menu-btn"/>
            <ui:Button name="Btn_Practice" text="PRACTICAR" class="menu-btn"/>
        </ui:VisualElement>
    </ui:VisualElement>
</ui:UXML>
